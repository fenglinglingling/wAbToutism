<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abs.mapper.CultureMapper">
    <select id="getAllCulture" resultType="culture_wenhua">
        select *
        from ab_culture_wenhua
    </select>
    <select id="getFourCulture" resultType="culture_wenhua">
        select *
        from ab_culture_wenhua limit 0,4
    </select>


</mapper>